<% if current_user %>
  Logged in as <%= current_user.full_name %>.
  <%= link_to "List Animals", user_path(current_user) %>
  <%= link_to "Log Out", logout_path %>
<% else %>
  <button><%= link_to "Sign Up", signup_path %></button>
  <button><%= link_to "Log In", login_path %></button>
  <%= button_to 'Sign in via Github', '/auth/github'%>
  <%= button_to 'Sign in via Facebook', '/auth/facebook'%>
  
<% end %>


<p id=”notice”><%= notice %></p>

<h1>Home#IndexPage</h1>
<h1> Welcome to Your New Animal Care Site </h1>

<p> This is an app where you can keep track of animals information. Medical records who owns what pet</p>


