<% if current_user %>
  Logged in as <%= current_user.full_name %>.
  <%= link_to "List Animals", user_path(current_user) %>
  <%= link_to "Log Out", logout_path %>
<% else %>
  <button><%= link_to "Sign Up", signup_path %></button>
  <button><%= link_to "Log In", login_path %></button>
    <%= link_to 'Login with Github', '/auth/github'%>
  <button><%= link_to('Log in with Facebook!', '/auth/facebook') %></button>
<% end %>


<p id=”notice”><%= notice %></p>

